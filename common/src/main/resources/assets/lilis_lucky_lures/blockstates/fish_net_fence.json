{
  "multipart": [
    {
      "apply": {
        "model": "lilis_lucky_lures:block/fish_net_fence_post"
      }
    },
    {
      "when": { "north": true },
      "apply": {
        "model": "lilis_lucky_lures:block/fish_net_fence_side",
        "uvlock": true
      }
    },
    {
      "when": { "east": true },
      "apply": {
        "model": "lilis_lucky_lures:block/fish_net_fence_side",
        "uvlock": true,
        "y": 90
      }
    },
    {
      "when": { "south": true },
      "apply": {
        "model": "lilis_lucky_lures:block/fish_net_fence_side",
        "uvlock": true,
        "y": 180
      }
    },
    {
      "when": { "west": true },
      "apply": {
        "model": "lilis_lucky_lures:block/fish_net_fence_side",
        "uvlock": true,
        "y": 270
      }
    }
  ]
}
